{
  "name": "FrameSeeker (OneUI) App",
  "short_name": "FrameSeeker",
  "start_url": "./",
  "scope": "./",
  "display": "fullscreen",
  "background_color": "#ffffff",
  "theme_color": "#0080ff",
  "dir": "ltr",
  "orientation": "portrait-primary",
  "lang": "en",
  "id": "FrameSeeker",
  "description": "A PWA (or possibly an actual app) that allows you to view and save frames of a video.",
  "share_target": {
    "action": "./videoIncoming",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "files": [
        {
          "name": "video",
          "accept": ["video/*"]
        }
      ]
    }
  },
  "icons": [
    {
      "src": "Icons/icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ]
}
